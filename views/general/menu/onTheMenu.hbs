{{> heroBanner}}
<div class="onTheMenu" id="onTheMenu">
    <div class="subTitle">
        <h6>Inspired by the freshest veggies, our chefs create unique recipes each and every
            week.</h6>
    </div>
    <div class="title">
        <h4>Classic Vegitarian Meals</h4>
    </div>
    <div id="container-menu" class="container-menu">
        {{#each food}}
        <div class="onTheMenuSmall">
            <img src="/uploads/{{imageUpload}}" alt="meal" />
            <div class="onTheMenuDesc">
                <p class="display-none" value="{{_id}}">{{_id}}</p>
                <h4>{{title}}</h4>
                <h6>{{ingredients}}</h6>
                <p id="onTheMenuCat">{{category}}</p>
                <hr>
                <p>{{description}}</p>
            </div>
        </div>
        {{/each}}
    </div>
</div>